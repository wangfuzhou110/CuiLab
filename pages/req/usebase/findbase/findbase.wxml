<!--pages/req/usebase/findbase/findbase.wxml-->
<scroll-view class='bases-container' scroll-y>
  <block class='bases' wx:for='{{bases}}' wx:key='name'>
    <view class='base' data-name='{{item.name}}' bindtap='onTapSelect' hover-class='hover-base' hover-start-time='0' hover-stay-time='0'>{{item.name}}</view>
  </block>
</scroll-view>
